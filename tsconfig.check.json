{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "lib": ["ES2022"],
    "types": ["node"],
    "skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
		"strict": true,
    "noEmit": true
  },
  "include": ["pi-extensions/**/*.ts", "scripts/**/*.ts"]
}
